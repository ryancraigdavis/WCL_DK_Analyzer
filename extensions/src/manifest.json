{
  "manifest_version": 3,
  "name": "WCL DK Analyzer for Cataclysm Classic",
  "description": "Tool to analyze WCL logs for WoW Cataclysm Classic Frost and Unholy DKs",
  "version": "2.0.0",
  "action": {
    "default_icon": "icon_128.png"
  },
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png"
  },
  "content_scripts": [{
    "js": ["content.js"],
    "matches": [
      "https://*.classic.warcraftlogs.com/*",
      "https://classic.warcraftlogs.com/*"
    ]
  }]
}
